import string
from AKModule1 import *

def Categorize(List):
    TK = []
    for z in range(len(List)):
        TK.append(List[z].lower())
    TK.sort()
    return TK
def Empty(List):
    AK = Categorize(List)
    TK = []
    for z in AK:
        if z != '':
            TK.append(z)
    return TK
def Count(List):
    TK = Empty(List)
    Words = []
    Counts = []
    for z in range(len(TK)):
        if z == 0:
            p = TK.count(TK[z])
            Words.append(TK[z])
            Counts.append(p)
        else:
            if TK[z] != TK[z-1]:
                p = TK.count(TK[z])
                Words.append(TK[z])
                Counts.append(p)
    UniqueWords = len(Words)        
    return Counts, Words, UniqueWords
