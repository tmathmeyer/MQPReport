from Module1 import *
import string

def Sort(List):
    LL = []
    for i in range(len(List)):
        LL.append(List[i].lower())
    LL.sort()
    return LL

def Null(List):
    L = Sort(List)
    LL = []
    for i in L:
        if i != '':
            LL.append(i)
    return LL

def Count(List):
    LL = Null(List)
    L = []
    X = []
    for i in range(len(LL)):
        if i == 0:
            x = LL.count(LL[i])
            L.append(LL[i])
            X.append(x)
        else:
            if LL[i] != LL[i-1]:
                x = LL.count(LL[i])
                L.append(LL[i])
                X.append(x)
    TotalDistinctWords = len(L)        
    return X, L, TotalDistinctWords
